pico-8 cartridge // http://www.pico-8.com
version 8
__lua__


function crosshair()
    local cross = {}
    cross.x = 64
    cross.y = 64
    cross.spr = 16
    return cross
end

function board()
    board = {max_x = 64, max_y = 64}
    local x = 0
    local y = 0
    for x=0,64,size do
        for y=0,64,size do
            add(board, {cells = {name=x.."-"..y, x=x, y=y}})
        end
    end
    --populatenumbers(board)
    return board; 
end


function listen()
    if (btnp(0)) cross.x -=size
    if (btnp(1)) cross.x +=size
    if (btnp(2)) cross.y -=size
    if (btnp(3)) cross.y +=size
    if (btnp(4) or btnp(5)) then 
        add(coroutines, cocreate(explode))
        sfx(1) 
    end
end

-- animate explosion
function explode()
    local sprite = 1
    local x = cross.x
    local y = cross.y
    while (sprite < 10) do
        cursor_enabled = false
        if (frame%3 == 1) sprite += 1
        spr(sprite, x, y)
        if (sprite == 3) add(marks, {marked = false, routined = false, x = x, y = y})
        yield()
    end
    cursor_enabled = true
    return true
end

function embers()
    for m in all(marks) do
        if (m.routined == false) then 
            m.routined = true
            local s = 32
            local x = m.x
            local y = m.y
            while (true) do
                if (frame%3 == 1) s += 1 
                spr(s, x, y)
                if (s >= 38) s = 32
                yield()
            end
        end     
    end
end

function fade_in_character(n,x,y)
    if n == 1 then 
        spr(17,x,y)
    elseif n == 2 then
    elseif n == 3 then
    end
end

function applymarks()
    for o in all(marks) do 
        if o.marked == false then
            -- if the board has been marked
            -- get a random number between 1 and 3
            number = flr(rnd(2)+1)
            fade_in_character(number, o.x, o.y)
            --add(coroutines, cocreate(embers))
        end
        o.marked = true
    end
end


function runcoroutines()
    for v in all(coroutines) do
        if (v != nil and costatus(v)) then
            coresume(v)
        elseif (v == nil) then
            del(coroutines, v)
        end
    end
end

function showcursor()
    if (cursor_enabled) then
        spr(cross.spr, cross.x, cross.y)
    end
end

function count_frames()
    frame += 1
    if (frame >= 30) frame = 0
end
 

 --[[ MAINS ]]--

function _init()
    frame = 0
    cross = {}
    cor = nil
    size = 8
    marks = {} -- markings on the floor where. places where we exploded stuff
    coroutines = {} -- coroutines for animations etc
    cursor_enabled = true
    cross = crosshair() 
    board = board()
end

function _update()
    count_frames()
    listen()
end


function _draw()
 cls()
 showcursor() 
 applymarks()
 runcoroutines()
 
end
